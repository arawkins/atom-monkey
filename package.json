{
  "name": "language-monkey2",
  "version": "1.4.0",
  "main": "./lib/monkey",
  "description": "Support for the Monkey2 language in Atom.",
  "keywords": [
    "monkey",
    "monkey2",
    "blitz",
    "brl",
    "basic"
  ],
  "repository": "https://github.com/arawkins/atom-monkey",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": "monkey:buildDefault",
    ".file.selected": "monkey:setCompilationTarget"
  },
  "activationHooks": [
    "language-monkey2:grammar-used"
  ],
  "dependencies": {
    "jquery": "^2.1.4",
    "node-dir": "^0.1.16"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provide"
      }
    }
  }
}
