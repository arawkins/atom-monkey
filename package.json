{
    "name": "language-monkey",
    "version": "0.1.0",
    "main": "./lib/monkey",
    "description": "Language support for Monkey programming languages.",
    "keywords": [
        "monkey",
        "monkey-x",
        "monkey2",
        "game programming language"
    ],
    "repository": "https://github.com/arawkins/atom-monkey",
    "license": "MIT",
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },
    "activationCommands": {
        "atom-workspace": "monkey:build",
        "atom-workspace": "monkey:buildDefault",
        ".file.selected": "monkey:setCompilationTarget"
    },
    "activationHooks": [
        "language-monkey:grammar-used",
        "language-monkey2:grammar-used"
    ],
    "dependencies": {
        "jquery": "^2.1.4",
        "atom-space-pen-views": "^2.2.0"
    }
}
